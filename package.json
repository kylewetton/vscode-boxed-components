{
	"name": "boxed-components",
	"displayName": "Boxed Components",
	"publisher": "kylewetton",
	"description": "Create a component folder structure including Storybook and styling",
	"repository": { "type": "git", "url": "https://github.com/kylewetton/vscode-boxed-components.git" },
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:boxed-components.createBox"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "boxed-components.createBox",
				"title": "Boxed Components [/] Create boxed component"
			}
		],
		"configuration": {
			"title": "Boxed Components",
			"properties": {
			  "boxed-components.useTemplates": {
				"type": "object",
				"default": {},
				"description": "User defined templates for Boxed Components"
			  }
			}
		  }
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.53.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"shelljs": "^0.8.4"
	}
}
